<core:FragmentDefinition
        xmlns="sap.m"
        xmlns:table="sap.ui.table"
        xmlns:core="sap.ui.core">
    <Dialog
    		id="siemensUiPopupDialog" 
    		title="{tablePopupView>/title}"
    		class="sapUiPopupWithPadding">
        <content>
            <table:Table 
            		id="siemensUiPopupTable" 
            		enableBusyIndicator="true">
                <table:toolbar>
                    <Toolbar 
                    		design="Transparent">
                        <Title 
                       		id="siemensUiTablePopupTitle"
                       		text="{tablePopupView>/subtitle}"/>
                    </Toolbar>
                </table:toolbar>
            </table:Table> 
        </content>
            <endButton>
                <Button 
                		text="{i18n>drilldown.Dialog.Button.Close}" 
                		press="onTableCellPopupClose" />
            </endButton>
    </Dialog>
</core:FragmentDefinition>